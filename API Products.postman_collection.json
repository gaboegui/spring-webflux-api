			"item": [
				{
					"name": "List products",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse the JSON response\r",
									"const response = pm.response.json();\r",
									"\r",
									"// Check if the response is an array and has at least one item\r",
									"if (Array.isArray(response) && response.length > 0) {\r",
									"    // Select the first product in the list\r",
									"    const firstProduct = response[0];\r",
									"    \r",
									"    // Save the ID to a collection variable named 'productId'\r",
									"    pm.collectionVariables.set(\"productId\", firstProduct.id);\r",
									"    pm.collectionVariables.set(\"categoryId\", firstProduct.category.id);\r",
									"    pm.collectionVariables.set(\"categoryName\", firstProduct.category.name);\r",
									"    \r",
									"    console.log(\"Saved Product ID:\", firstProduct.id);\r",
									"} else {\r",
									"    console.warn(\"No products found in the response!\");\r",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "localhost:8080/api/products",
							"host": [
								"localhost"
							],
							"port": "8080",
							"path": [
								"api",
								"products"
							]
